# Render Ingestion Service Environment Variables

# API Key for authentication (must match SAP Agent)
# Generate with: python -c "import secrets; print(secrets.token_urlsafe(32))"
API_KEY=your-api-key-here

# Fernet encryption key (must match SAP Agent)
# Generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
ENCRYPTION_KEY=your-encryption-key-here

# Supabase Configuration (REQUIRED for database operations)
# Supabase project URL (from Supabase Dashboard → Project Settings → API)
SUPABASE_URL=https://your-project-ref.supabase.co

# Supabase service role key (from Supabase Dashboard → Project Settings → API)
# This is the JWT token that provides admin-level database access
# WARNING: This key bypasses Row Level Security (RLS) - keep it secret!
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Legacy DATABASE_URL (kept for backward compatibility but not used by Supabase client)
# Format: postgresql://postgres:[password]@[host]:5432/postgres
DATABASE_URL=postgresql://postgres:your-password@db.your-project.supabase.co:5432/postgres

# Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Port (optional, defaults to 8080)
PORT=8080
